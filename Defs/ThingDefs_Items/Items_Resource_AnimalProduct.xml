<?xml version="1.0" encoding="utf-8" ?>

<Defs>
    <!-- Muffaloaf -->
    <ThingDef ParentName="EggUnfertBase">
        <defName>DRS_EggMuffaloafUnfertilized</defName>
        <label>chicken egg (unfert.)</label>
        <description>An unfertilized chicken egg. It can be eaten raw, but it's much, much better cooked.</description>
        <graphicData>
          <color>(228,185,135)</color>
        </graphicData>
        <statBases>
          <MarketValue>7.0</MarketValue>
        </statBases>
      </ThingDef>
    
      <ThingDef ParentName="EggFertBase">
        <defName>DRS_EggMuffalofFertilized</defName>
        <label>chicken egg (fert.)</label>
        <description>A fertilized Muffaloaf egg. If all goes well, it should hatch into a Muffachick. It can be eaten raw, but it's much better cooked.</description>
        <possessionCount>1</possessionCount>
        <graphicData>
          <color>(245,245,245)</color>
        </graphicData>
        <statBases>
          <MarketValue>7.0</MarketValue>
        </statBases>
        <comps>
          <li Class="CompProperties_Hatcher">
            <hatcherDaystoHatch>3.5</hatcherDaystoHatch>
            <hatcherPawn>DRS_Muffaloaf</hatcherPawn>
          </li>
        </comps>
      </ThingDef>
</Defs>